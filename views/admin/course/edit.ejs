<%- include("../header.ejs") %>
<%- include("../sidebar.ejs") %>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">Edit course Form</h1>
                </div>
                <!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Display</li>
                    </ol>
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- </div> -->
    <div class="card">
        <!-- /.card-header -->
        <div class="card-body">
            <table id="example1" class="table table-bordered table-striped">
                
                <div class="container-fluid">
                    <form
                    action="/admin/courseupdate/<%= d.id %>" method="post"
                  >
                 
                    <div class="mb-3">
                      <label for="">name</label>
                     
                      <input type="text" class="form-control" placeholder="Enter full name" name="name"
                      id="name" required value="<%= d.name %>" />
                      <label for="">Email</label>
                      <input type="email" name="email" class="form-control" placeholder="Enter email" name="email" id="email" value="<%= d.email %>" />
                   
          
                    
                      <center>
                        <button class="btn btn-info" type="submit" style="margin-top: 25px;">Submit</button>
                      </center>
                    </div>
                  </form>

                </div>
                <br />
                <center>
                    <a href="/admin/coursedisplay" class="btn btn-primary">Back</a>
                </center>
                <br />
            </table>
        </div>
        <!-- /.card-body -->
    </div>
    <!-- /.card -->
</div>
<!-- /.content-wrapperÂ -->

<%- include("../footer.ejs") %>